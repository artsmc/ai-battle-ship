{
  "name": "battleship-naval-strategy",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "db:init": "./scripts/db/init-database.sh",
    "db:init:basic": "./scripts/db/init-database.sh --basic-schema",
    "db:seed": "psql $DATABASE_URL -f scripts/db/seed.sql",
    "db:seed:ships": "psql $DATABASE_URL -f scripts/db/seed-ship-types.sql",
    "db:generate": "tsx scripts/db/generate-test-data.ts",
    "db:reset": "./scripts/db/init-database.sh --skip-test-data",
    "db:backup": "./scripts/db/backup-restore.sh backup-full",
    "db:restore": "./scripts/db/backup-restore.sh restore",
    "db:manage": "./scripts/db/backup-restore.sh",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v",
    "analyze": "ANALYZE_BUNDLE=true next build",
    "clean": "rm -rf .next node_modules package-lock.json && npm install",
    "check-all": "npm run type-check && npm run lint && npm run format:check"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-query": "^5.51.0",
    "@types/jsonwebtoken": "^9.0.10",
    "bcrypt": "^5.1.1",
    "electric-sql": "^0.12.0",
    "framer-motion": "^12.23.12",
    "jsonwebtoken": "^9.0.2",
    "konva": "^9.2.0",
    "lucide-react": "^0.544.0",
    "next": "^14.2.0",
    "pg": "^8.11.3",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-hook-form": "^7.62.0",
    "react-konva": "^18.2.10",
    "uuid": "^9.0.1",
    "wa-sqlite": "^0.9.9",
    "zod": "^3.22.4",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@playwright/test": "^1.55.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.14.0",
    "@types/pg": "^8.11.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "lint-staged": "^15.2.0",
    "playwright": "^1.55.0",
    "postcss": "^8.4.0",
    "prettier": "^3.3.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.1",
    "typescript": "^5.5.0"
  }
}
